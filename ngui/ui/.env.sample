# Local development only
# Name of the ngui app build directory
# Defaults to "build"
VITE_BUILD_DIR=

# Local development only
# Cluster URL to proxy API requests to
VITE_PROXY=

# Local development only
# Apollo http server URL
VITE_APOLLO_HTTP_BASE=

# Local development only
# Apollo ws server URL
VITE_APOLLO_WS_BASE=

# Local development only
# Running vite app locally. Defaults to 3000
VITE_PORT=

# Local development only
# Running vite preview server locally. Defaults to 4000
VITE_PREVIEW_PORT=

# Local development only
# Set to "true" to allow Vite to listen on all interfaces (e.g., 0.0.0.0),
# "false" to restrict to localhost,
# or a specific host string (e.g., "localhost" or "0.0.0.0")
# Defaults to false if unset
VITE_HOST=

# Allowed hosts during local development:
# - true: allow all (not recommended; vulnerable to DNS rebinding attacks)
# - comma-separated list: allow specific hosts (e.g. localhost,127.0.0.1,mydomain.dev)
# If unset, Vite will not set the allowedHosts option.
# Example: VITE_ALLOWED_HOSTS=localhost,127.0.0.1,mydomain.dev
VITE_ALLOWED_HOSTS=

# OAuth Client ID to auth via Google
VITE_GOOGLE_OAUTH_CLIENT_ID=

# OAuth Client ID to auth via Microsoft
VITE_MICROSOFT_OAUTH_CLIENT_ID=

# Google Map API key
VITE_GOOGLE_MAP_API_KEY=

# Google Analytics tracking code (ID)
VITE_GANALYTICS_ID=

# Hotjar ID
VITE_HOTJAR_ID=

# Defines how users can join an organization on app initialization
# "automatic" (default) - a new organization is automatically created for the user unless invited to an existing organization
# "invite-only" - users are only allowed to accept invitations and join an existing organization
VITE_ON_INITIALIZE_ORGANIZATION_SETUP_MODE="automatic"

# Defines if the FinOps portal overview is enabled
# "enabled" - the overview is enabled
# otherwise - the overview is disabled
VITE_FINOPS_IN_PRACTICE_PORTAL_OVERVIEW=